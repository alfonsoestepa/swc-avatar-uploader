{
  "name": "swc-avatar-uploader",
  "version": "1.0.0",
  "description": "Avatar uploader webcomponent",
  "main": "index.html",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "serve": "polymer serve -o",
    "test": "polymer test --skip-selenium-install",
    "clean:deps": "rm -rf node_modules && rm -rf bower_components",
    "analyze": "polymer analyze swc-avatar-uploader.html > analysis.json",
    "poly-lint": "polylint -i swc-avatar-uploader.html --no-recursion",
    "lint": "npm run lint:javascript && polymer lint",
    "lint:javascript": "eslint . --ext js,html",
    "postinstall": "bower install"
  },
  "repository": {
    "type": "git",
    "url": "https://innersource.soprasteria.com/abraham.chinarroterren/swc-avatar-uploader.git"
  },
  "author": "soprasteria developers",
  "license": "ISC",
  "devDependencies": {
    "eslint": "^3.11.1",
    "eslint-plugin-html": "^1.7.0",
    "eslint-config-google": "^0.7.0",
    "polylint": "^2.10.1",
    "polyserve": "^0.12.0",
    "web-component-tester": "^4.0.0",
    "web-component-tester-istanbul": "^0.10.0"
  }
}